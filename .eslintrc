{
  "extends": "airbnb",
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "global-require": 0,
    "import/no-dynamic-require": 0,
    "no-underscore-dangle": ["error", { "allow": [ "_id" ] }],
    "array-bracket-spacing": 0,
    "curly": 0,
    "semi": 0,
    "arrow-parens": ["error", "as-needed", { "requireForBlockBody": false }]
  }
}
